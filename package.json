{
  "name": "sparql-query-ui",
  "version": "0.1.0",
  "description": "Modern SPARQL query interface built with Svelte 5",
  "type": "module",
  "main": "./package/index.js",
  "module": "./package/index.js",
  "types": "./package/index.d.ts",
  "exports": {
    ".": {
      "types": "./package/index.d.ts",
      "svelte": "./package/index.js",
      "default": "./package/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "package",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "svelte": "./package/index.js",
  "scripts": {
    "dev": "vite",
    "build": "npm run type-check && vite build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:standalone": "npm run build && npm run copy-standalone",
    "copy-standalone": "node scripts/copy-standalone.js",
    "package": "svelte-package -o package",
    "preview": "vite preview",
    "preview:standalone": "vite preview --outDir dist-standalone",
    "demo": "node server-demo.js",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:storybook": "playwright test --config=playwright-storybook.config.js",
    "test:e2e:storybook:ui": "playwright test --config=playwright-storybook.config.js --ui",
    "test:e2e:storybook:debug": "playwright test --config=playwright-storybook.config.js --debug",
    "test:e2e:standalone": "playwright test --config=playwright-standalone.config.js",
    "test:e2e:standalone:ui": "playwright test --config=playwright-standalone.config.js --ui",
    "test:e2e:standalone:debug": "playwright test --config=playwright-standalone.config.js --debug",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src tests",
    "format:check": "prettier --check src tests",
    "check": "npm run type-check && npm run lint && npm run format:check && npm run check:cdn && npm run test:unit && npm run build-storybook",
    "check:cdn": "node scripts/check-cdn-urls.js",
    "check:external": "node scripts/check-external-deps.js",
    "prepublishOnly": "npm run check && npm test && npm run package",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --quiet",
    "test:storybook": "test-storybook",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "keywords": [
    "sparql",
    "rdf",
    "semantic-web",
    "query",
    "svelte",
    "svelte-component",
    "web-component",
    "carbon",
    "carbon-design-system",
    "yasgui",
    "typescript",
    "query-editor",
    "linked-data"
  ],
  "author": "Arne Bernhardt",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/sparql-query-ui.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/sparql-query-ui/issues"
  },
  "homepage": "https://github.com/yourusername/sparql-query-ui#readme",
  "peerDependencies": {
    "carbon-components-svelte": "^0.90.0",
    "carbon-icons-svelte": "^13.0.0",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@eslint/js": "^9.39.0",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^10.0.2",
    "@storybook/addon-docs": "^10.0.2",
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/addon-vitest": "^10.0.2",
    "@storybook/sveltekit": "^10.0.2",
    "@storybook/test-runner": "^0.24.1",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.8",
    "@tsconfig/svelte": "^5.0.5",
    "@types/n3": "^1.26.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "carbon-preprocess-svelte": "^0.11.12",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.0",
    "eslint-plugin-storybook": "^10.0.2",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook": "^10.0.2",
    "svelte": "^5.43.2",
    "svelte-eslint-parser": "^1.4.0",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-bundle-visualizer": "^1.2.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.19.1",
    "@codemirror/commands": "^6.10.0",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-xml": "^6.1.0",
    "@codemirror/language": "^6.11.3",
    "@codemirror/lint": "^6.9.2",
    "@codemirror/search": "^6.5.11",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.38.6",
    "codemirror": "^6.0.2",
    "n3": "^1.26.0",
    "wx-svelte-grid": "^2.3.0"
  },
  "peerDependenciesMeta": {
    "carbon-components-svelte": {
      "optional": false
    },
    "carbon-icons-svelte": {
      "optional": false
    }
  }
}
